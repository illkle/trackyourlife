{
  "name": "trackyourlife",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "check": "pnpm lint && pnpm lint:ws && pnpm typecheck && pnpm test",
    "db:studio": "turbo run studio -F @tyl/db",
    "db:generate": "turbo run generate -F @tyl/db",
    "db:migrate": "turbo run migrate -F @tyl/db",
    "db:push": "turbo run push -F @tyl/db",
    "dev": "turbo watch dev",
    "dev:web": "turbo watch dev -F @tyl/server",
    "dev:mobile": "turbo watch dev -F @tyl/mobile",
    "dev:db": "turbo watch dev -F @tyl/db",
    "format": "oxfmt",
    "infra:up": "docker compose -f docker/docker-compose-dev.yml up -d --build",
    "infra:down": "docker compose -f docker/docker-compose-dev.yml down",
    "infra:logs": "docker compose -f docker/docker-compose-dev.yml logs -f",
    "lint": "turbo run lint",
    "lint:fix": "oxlint . --fix",
    "lint:ws": "sherif",
    "lint:ws:fix": "sherif --fix",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e -F @tyl/server",
    "test:e2e:ui": "turbo run test:e2e -F @tyl/server -- --ui",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@turbo/gen": "2.8.10",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.49.0",
    "sherif": "1.10.0",
    "turbo": "2.8.10",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "packageManager": "pnpm@10.28.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@journeyapps/wa-sqlite",
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "core-js-pure",
      "esbuild",
      "unrs-resolver"
    ],
    "patchedDependencies": {
      "@tanstack/powersync-db-collection": "patches/@tanstack__powersync-db-collection.patch"
    }
  }
}
