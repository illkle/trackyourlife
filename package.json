{
  "name": "trackyourlife",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:studio": "turbo run studio -F @tyl/db",
    "db:generate": "turbo run generate -F @tyl/db",
    "dev": "turbo watch dev",
    "format": "oxfmt",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "lint:ws": "pnpm dlx sherif@latest",
    "lint:ws:fix": "pnpm dlx sherif@latest --fix",
    "postinstall": "pnpm lint:ws",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "test:e2e:ui": "turbo run test:e2e -- --ui",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@turbo/gen": "2.7.6",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.49.0",
    "turbo": "2.7.6",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "packageManager": "pnpm@10.28.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@journeyapps/wa-sqlite",
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "core-js-pure",
      "esbuild",
      "unrs-resolver"
    ],
    "patchedDependencies": {
      "@tanstack/powersync-db-collection": "patches/@tanstack__powersync-db-collection.patch"
    }
  }
}