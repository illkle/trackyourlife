{
  "name": "@tyl/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./src/server/index.ts"
    },
    "./server/*": {
      "types": "./dist/server/*.d.ts",
      "default": "./src/server/*.ts"
    },
    "./client/*": {
      "types": "./dist/client/*.d.ts",
      "default": "./src/client/*.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "oxlint .",
    "generate": "pnpm with-env drizzle-kit generate",
    "generate:manual-migration": "pnpm with-env drizzle-kit generate --custom",
    "up": "pnpm with-env drizzle-kit up",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "migrate:deploy": "pnpm drizzle-kit migrate",
    "push": "pnpm with-env drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@powersync/common": "catalog:",
    "@rocicorp/zero": "catalog:",
    "drizzle-orm": "catalog:",
    "drizzle-zod": "^0.8.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tyl/prettier-config": "workspace:*",
    "@tyl/tsconfig": "workspace:*",
    "@types/pg": "^8.15.6",
    "dotenv-cli": "catalog:",
    "drizzle-kit": "catalog:",
    "pg": "^8.16.3",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@tyl/prettier-config"
}