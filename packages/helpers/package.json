{
  "name": "@tyl/helpers",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    },
    "./color/*": {
      "types": "./dist/color/*.d.ts",
      "default": "./src/color/*.ts"
    },
    "./data/*": {
      "types": "./dist/data/*.d.ts",
      "default": "./src/data/*.ts"
    },
    "./data/TrackableFlagsProvider": {
      "types": "./dist/data/TrackableFlagsProvider.d.ts",
      "default": "./src/data/TrackableFlagsProvider.tsx"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf .turbo node_modules",
    "lint": "oxlint .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@date-fns/tz": "catalog:",
    "@fluentui/react-context-selector": "^9.2.13",
    "@powersync/drizzle-driver": "catalog:",
    "chroma-js": "^3.1.2",
    "date-fns": "catalog:",
    "drizzle-orm": "catalog:",
    "fuzzysort": "^3.1.0",
    "lodash-es": "^4.17.21",
    "uuid": "catalog:"
  },
  "devDependencies": {
    "@tyl/db": "workspace:*",
    "@tyl/tsconfig": "workspace:*",
    "@types/chroma-js": "^3.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "catalog:react",
    "react": "catalog:react",
    "typescript": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:react",
    "zod": "catalog:"
  }
}