{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".expo/**",
        ".output/**",
        "dist/**"
      ],
      "env": [
        "VITE_DEPLOY_DOMAIN",
        "VITE_POWERSYNC_DOMAIN",
        "VITE_PREVIEW_WARNING",
        "VITE_ENABLE_DEVTOOLS"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json",
        "**/*.tsbuildinfo"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true,
      "env": [
        "DATABASE_URL"
      ]
    },
    "studio": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL"
      ]
    },
    "generate": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "migrate": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "up": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "test": {
      "cache": false
    },
    "test:e2e": {
      "cache": false
    },
    "migrate:deploy": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    }
  },
  "globalEnv": [
    "NODE_ENV"
  ]
}
